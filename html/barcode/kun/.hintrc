{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "highest-available-document-mode": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "zoom",
          "-webkit-overflow-scrolling",
          "backdrop-filter"
        ]
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "axe/time-and-media": [
      "default",
      {
        "meta-refresh": "off"
      }
    ],
    "button-type": "off",
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/layout": "off",
    "detect-css-reflows/paint": "off"
  }
}