{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "zoom",
          "user-select",
          "-webkit-overflow-scrolling",
          "backdrop-filter",
          "accent-color"
        ]
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "highest-available-document-mode": "off",
    "axe/time-and-media": [
      "default",
      {
        "meta-refresh": "off"
      }
    ]
  }
}