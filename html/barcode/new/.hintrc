{
  "extends": [
    "development"
  ],
  "hints": {
    "highest-available-document-mode": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ],
    "no-inline-styles": "off",
    "axe/time-and-media": [
      "default",
      {
        "meta-refresh": "off"
      }
    ]
  }
}